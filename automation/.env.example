# Deployment Configuration
# Environment type (e.g., 'dev-' for development)
ENV_TYPE=
# Environment suffix (e.g., '-0' for instance numbering)
ENV_SUFFIX=
# Path to Tailscale auth key file
TSAUTHKEY_PATH=./.secrets/n8n-tsauthkey.key
# Set to true if tsauthkey is managed externally
TSAUTHKEY_EXTERNAL=false

# n8n Core Settings
# Port for n8n service (default: 5678)
N8N_PORT=5678
# Host for n8n service (default: localhost)
# N8N_HOST=localhost
# Base URL for n8n (e.g., https://n8n.example.com)
# N8N_PROTOCOL=https
# WEBHOOK_URL=https://n8n.example.com/webhook
# Path for n8n endpoints (default: /)
# N8N_PATH=/

# n8n Security Settings
# JWT secret for authentication
# N8N_AUTH_JWT_SECRET=your_jwt_secret
# Enable basic auth (true/false)
# N8N_BASIC_AUTH_ACTIVE=false
# Basic auth username
# N8N_BASIC_AUTH_USER=admin
# Basic auth password
# N8N_BASIC_AUTH_PASSWORD=your_password

# n8n Database Settings
# Database type (default: sqlite)
# DB_TYPE=sqlite
# SQLite database path
# DB_SQLITE_PATH=/home/node/.n8n/database.sqlite
# PostgreSQL/MySQL database settings (if DB_TYPE is postgres or mysql)
# DB_POSTGRESDB_HOST=localhost
# DB_POSTGRESDB_PORT=5432
# DB_POSTGRESDB_DATABASE=n8n
# DB_POSTGRESDB_USER=n8n
# DB_POSTGRESDB_PASSWORD=your_db_password

# n8n Performance Settings
# Number of workers for queue mode
# QUEUE_WORKER_COUNT=1
# Maximum number of executions to keep
# EXECUTIONS_DATA_MAX_AGE=336
# Execution timeout in seconds
# EXECUTIONS_TIMEOUT=3600

# n8n Integration Settings
# URL for external services (e.g., AI models)
# N8N_AI_URL=https://api.example.com
# API key for external services
# N8N_AI_API_KEY=your_api_key