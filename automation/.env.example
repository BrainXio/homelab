# Deployment Configuration
# Environment type (e.g., '-dev' for development)
ENV_TYPE=-dev
# Environment suffix (e.g., '-0' for instance numbering)
ENV_SUFFIX=-0
# Path to Tailscale auth key file
TSAUTHKEY_PATH=./tsauthkey.key
# Use external secret for tsauthkey (true/false)
TSAUTHKEY_EXTERNAL=false

# n8n Core Settings
# Port for n8n web interface (default: 5678)
N8N_PORT=5678
# Protocol for n8n (http or https)
N8N_PROTOCOL=https
# Node.js environment (e.g., production, development)
NODE_ENV=production
# Webhook URL for n8n (e.g., https://a8n-workflow.example.com)
WEBHOOK_URL=https://a8n-workflow.example.com
# Timezone for n8n (e.g., Europe/Berlin)
GENERIC_TIMEZONE=Europe/Berlin

# n8n Features
# Enable diagnostics (true/false)
N8N_DIAGNOSTICS_ENABLED=false
# Enable version notifications (true/false)
N8N_VERSION_NOTIFICATIONS_ENABLED=false
# # Enable community nodes (true/false)
# N8N_COMMUNITY_PACKAGES_ENABLED=true
# # Enable external storage for large payloads (e.g., s3, redis)
# N8N_PAYLOAD_SIZE_MAX=16
# # Enable multi-user mode (true/false)
# N8N_PERSONALIZATION_ENABLED=true

# Security
# # JWT secret for authentication
# N8N_AUTH_JWT_SECRET=
# # Encryption key for sensitive data
# N8N_ENCRYPTION_KEY=
# # Disable UI (true/false)
# N8N_DISABLE_UI=false
# # Restrict endpoints (comma-separated list)
# N8N_BLOCKED_ENDPOINTS=

# Database
# # Database type (sqlite, postgres, mysql, mariadb)
# N8N_DB_TYPE=sqlite
# # Database connection string (if not sqlite)
# DB_POSTGRESQL_CONNECTION_STRING=
# # SQLite database path
# N8N_DB_SQLITE_PATH=/home/node/.n8n/database.sqlite

# Performance
# # Maximum concurrent executions
# EXECUTIONS_PROCESS=main
# # Execution timeout (seconds)
# EXECUTIONS_TIMEOUT=-1
# # Maximum execution data size (MB)
# EXECUTIONS_DATA_MAX_SIZE=512
# # Queue mode (true/false)
# N8N_QUEUE_MODE_ENABLED=false
# # Redis connection string for queue mode
# QUEUE_BULL_REDIS_URL=

# Logging
# # Log level (error, warn, info, debug, verbose)
# N8N_LOG_LEVEL=info
# # Log output (console, file)
# N8N_LOG_OUTPUT=console
# # Log file path
# N8N_LOG_FILE_LOCATION=/home/node/.n8n/n8n.log

# Miscellaneous
# # Custom config file path
# N8N_CONFIG_FILES=
# # Enable push connections for real-time updates
# N8N_PUSH_BACKEND=sse
# # Enable experimental features (comma-separated list)
# N8N_EXPERIMENTAL_FEATURES=
# # Proxy settings (http://user:pass@host:port)
# N8N_PROXY_URL=
# # SSL certificates for HTTPS
# N8N_SSL_KEY=
# N8N_SSL_CERT=